{
    "name": "acmephp/symfony-bundle",
    "description": "AcmePhpBundle",
    "keywords": [
        "certificate",
        "letencrypt",
        "https",
        "acmephp",
        "symfony",
        "bundle"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Titouan Galopin",
            "email": "galopintitouan@gmail.com",
            "homepage": "http://titouangalopin.com"
        },
        {
            "name": "Jérémy Derussé",
            "homepage": "https://twitter.com/jderusse"
        }
    ],
    "autoload": {
        "psr-4": {
            "AcmePhp\\Bundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TestAppBundle\\": "features/fixtures/TestApp/TestAppBundle"
        }
    },
    "require": {
        "php": ">=5.5.0",
        "symfony/framework-bundle": "^2.6|^3.0",
        "psr/log": "^1.0.0",
        "symfony/console": "^2.3|^3.0",
        "symfony/filesystem": "^2.3|^3.0",
        "symfony/dependency-injection": "^2.7|^3.0",
        "symfony/finder": "^2.3|^3.0",
        "acmephp/core": "1.0.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "symfony/process": "^2.5|^3.0",
        "sllh/php-cs-fixer-styleci-bridge": "^1.5",
        "behat/behat": "~3.1@dev",
        "behat/symfony2-extension": "~2.0",
        "symfony/property-access": "^3.0"
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
